{
  "name": "pcln-icons-perf",
  "version": "1.0.0",
  "description": "Priceline React icons based on Material Design Icons",
  "main": "lib/Icon.js",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build:svg && npm run build:index && npm run build:copy && npm run build:babel",
    "clean": "rm -rf node_modules components lib",
    "build:svg": "npx @svgr/cli svg -d components",
    "build:index": "node ./create-index.js",
    "build:copy": "cp src/Svg.js src/index.js components",
    "build:babel": "npx @babel/cli components -d lib",
    "test": "jest"
  },
  "author": "Priceline",
  "license": "MIT",
  "dependencies": {
    "lodash.upperfirst": "^4.3.1",
    "styled-system": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.3",
    "@svgr/cli": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6"
  },
  "peerDependencies": {
    "styled-components": ">=3.x || >=4.x"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "../../test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  }
}
